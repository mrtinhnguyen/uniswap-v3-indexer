{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/update-config",
      "dest": "/api/update-config.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

