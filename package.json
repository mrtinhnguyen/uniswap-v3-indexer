{
  "name": "uni_v3",
  "version": "0.1.0",
  "scripts": {
    "clean": "gts clean",
    "mocha": "ts-mocha test/**/*.ts",
    "codegen": "envio codegen",
    "dev": "node scripts/update-config.js && envio dev",
    "test": "pnpm mocha",
    "start": "node scripts/update-config.js && node scripts/ensure-generated.js && envio start",
    "start:full": "node scripts/start-with-setup.js",
    "update-config": "node scripts/update-config.js",
    "update-alerts-config": "node scripts/update-alerts-config.js",
    "watch-pools": "node scripts/watch-pools.js",
    "check-env": "node scripts/check-env.js",
    "setup-db-env": "node scripts/setup-db-env.js",
    "test-telegram": "node scripts/test-telegram-bot.js",
    "db-setup": "cd generated && pnpm run db-setup",
    "db-up": "cd generated && pnpm run db-up",
    "lint": "gts lint",
    "fix": "gts fix",
    "posttest": "pnpm run lint",
    "render:build": "pnpm install && pnpm run codegen",
    "render:start": "pnpm run start"
  },
  "devDependencies": {
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.10.1",
    "chai": "^6.2.1",
    "gts": "^6.0.2",
    "mocha": "^11.7.5",
    "ts-mocha": "^11.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "envio": "^2.32.2",
    "js-yaml": "^4.1.0",
    "viem": "^2.39.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  }
}
